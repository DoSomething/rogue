{
    "gitbook": "3.2.3",
    "title": "Rogue Documentation",
    "root": "./docs",
    "structure.glossary": "GLOSSARY.md",
    "structure.readme": "README.md",
    "structure.summary": "SUMMARY.md",
    "plugins": [
        "-highlight",
        "anchorjs",
        "collapsible-menu",
        "editlink",
        "github",
        "prism"
    ],
    "pluginsConfig": {
        "editlink": {
            "base": "https://github.com/DoSomething/rogue/tree/master/docs",
            "label": "Edit This Page"
        },
        "github": {
            "url": "https://github.com/DoSomething/rogue"
        }
    }
}
